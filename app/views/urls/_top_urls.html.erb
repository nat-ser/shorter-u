<table class="table table-hover top-urls">
  <thead>
    <tr>
      <th>Url</th>
      <th>Visit Count</th>
    </tr>
  </thead>
  <tbody>
    <% @top_urls.each do |url| %>
      <tr>
        <td><%= url.full_address %></td>
        <td><%= url.visit_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
